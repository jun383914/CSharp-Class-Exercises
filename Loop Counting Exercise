using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Countloop
{
    class Program
    {
        static void Main(string[] args)
        {
            int m = 1;
            while (m <= 5)
            {
                m++;

                for (int i = 1; i <= 10; i++)
                {

                    Console.WriteLine(i);
                }
                for (int n = 10; n >= 1; n--)
                {
                    Console.WriteLine(n);
                }
            }
        }
    }
}
